{
  "mount": {
    "public": "/",
    "src": "/_dist_"
  },
  "plugins": [
    [
      "@snowpack/plugin-optimize"
    ],
    [
      "snowpack-plugin-import-replacer", {
        "modules": {
          "three": "https://cdn.skypack.dev/pin/three@v0.117.1-NetLzdTnw9ga3y6o633U/min/three.js"
        },
        "extensions": [".js"]
      }
    ]
  ],
  "installOptions": {
    "namedExports": [
      "@enable3d/ammo-physics"
    ],
    "polyfillNode": true
  }
}
