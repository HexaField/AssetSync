{
    "name": "@AssetSync/AssetSync",
    "version": "0.0.0",
    "main": "src/index.js",
    "author": "HexaField <joshfield999@gmail.com>",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "setup": "yarn install",
        "update": "yarn upgrade",
        "fix": "yarn audit fix",
        "test": "ava",
        "clean:build": "rm -rf dist",
        "build": "rollup src/index.js --format umd --name '@AssetSync/AssetSync' --file dist/index.js --inlineDynamicImports"
    },
    "ava": {
        "files": [
            "test/*.spec.js"
        ],
		"concurrency": 1,
		"verbose": true,
        "nodeArguments": [
            "--harmony-top-level-await"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "ava": "^3.13.0",
        "multiaddr": "^8.0.0"
    },
    "dependencies": {
        "@AssetSync/common": "^0.0.0",
        "@forlagshuset/simple-fs": "^0.4.0",
        "datastore-fs": "^2.0.2",
        "datastore-level": "^2.0.0",
        "file-type": "^14.7.1",
        "fs-extra": "^9.0.1",
        "hyperdiff": "^2.0.7",
        "it-pipe": "^1.1.0",
        "lodash": "^4.17.20",
        "lodash.clonedeep": "^4.5.0",
        "peer-id": "^0.14.2",
        "platform": "^1.3.6",
        "simple-peer": "^9.9.3",
        "@geckos.io/typed-array-buffer-schema": "^1.0.2"
    }
}
